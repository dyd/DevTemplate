<!DOCTYPE html>
<html lang="en" xmlns="http://www.w3.org/1999/html">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>{{ get_Translation('Common', 'title') }}</title>
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="{{ base_url() }}/img/favicon.ico">
    <!-- Bootstrap -->
    <link href="{{ base_url() }}/vendor/elaAdmin/css/bootstrap.min.css" rel="stylesheet">
    <link href="{{ base_url() }}/vendor/elaAdmin/css/helper.css" rel="stylesheet">
    <link href="{{ base_url() }}/vendor/elaAdmin/css/style.css" rel="stylesheet">
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script type="text/javascript" src="{{ base_url() }}/vendor/html5shiv/html5shiv.min.js"></script>
    <script type="text/javascript" src="{{ base_url() }}/vendor/respond/respond.min.js"></script>
    <![endif]-->
    <!-- Sweetalert -->
    <link href="{{ base_url() }}/vendor/swal/sweetalert.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="{{ base_url() }}/vendor/font-awesome-4.7.0/css/font-awesome.css" rel="stylesheet">
    <!-- Custom css -->
    <link href="{{ base_url() }}/css/main.css" rel="stylesheet">
</head>
<body>
<div id="spinner" style="display: none;"></div>

{% include 'templates/partial/modal.twig' %}

{% block content %}{% endblock %}

<footer class="footer">
    <div class="container-fluid text-center">

        {{ get_Translation('Footer', 'footerText') }}

    </div>
</footer>

<!-- Jquery -->
<script src="{{ base_url() }}/vendor/jquery/jquery-3.3.1.min.js"></script>
<!-- ElaAdmin -->
<script src="{{ base_url() }}/vendor/elaAdmin/js/popper.min.js"></script>
<script src="{{ base_url() }}/vendor/elaAdmin/js/bootstrap.min.js"></script>
<!-- Civem -->
<script src="{{ base_url() }}/vendor/civem/civem.js"></script>
<!-- Sweetalert -->
<script src="{{ base_url() }}/vendor/swal/sweetalert.min.js" type="text/javascript"></script>
<!-- Utils -->
<script src="{{ base_url() }}/js/utils.js"></script>
<script>
    var base_url = "{{ base_url() }}";
    var base_self = "{{ current_path() }}";
    $(function () {
        $('[data-toggle="popover"]').popover();
    });
</script>

{% block javascript %}{% endblock %}

</body>
</html>